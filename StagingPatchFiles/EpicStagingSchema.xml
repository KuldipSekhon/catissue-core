		</column>	
	</createTable>
	
	<createTable tableName="STAGING_PART_ID_HISTORY">
		<column name="PART_SOURCE_ID" type="${text.type}(250)">
		</column>
		<column name="PART_SOURCE" type="${text.type}(250)">
		</column>
